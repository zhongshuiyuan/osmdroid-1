<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ComTitle">
        <attr name="left_image" format="reference" />
        <attr name="right_image" format="reference" />
        <attr name="right_visable" format="boolean" />
        <attr name="right_title_name" format="string" />
        <attr name="left_visable" format="boolean" />
        <attr name="left_title_name" format="string" />
        <attr name="left_action" format="boolean" />
        <attr name="title_name" format="string" />
    </declare-styleable>
    <declare-styleable name="ActionBar">
        <attr name="title" format="string" localization="suggested" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension"></attr>
        <attr name="border_color" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="SwipeMenuView">
        <attr name="swipeEnable" format="boolean" />
        <attr name="ios" format="boolean" />
        <attr name="leftSwipe" format="boolean" />
    </declare-styleable>
</resources>