<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".project.recordbook.bookfragment.LargeMediumBridgeRecordBookFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_10_dp"
        android:background="@drawable/shape_input_bg_white"
        android:orientation="vertical"
        android:paddingLeft="@dimen/dimen_8_dp"
        android:paddingTop="@dimen/dimen_10_dp"
        android:paddingRight="@dimen/dimen_8_dp"
        android:paddingBottom="@dimen/dimen_15_dp">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dimen_10_dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="桥梁名称"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_qlmc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:gravity="center_vertical"
                    android:maxLength="32"
                    android:padding="@dimen/dimen_5_dp"
                    android:singleLine="true"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="地名（河名）"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_dmhm"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:gravity="center_vertical"
                    android:maxLength="32"
                    android:padding="@dimen/dimen_5_dp"
                    android:singleLine="true"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_15_dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dimen_10_dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="桥梁中心桩号"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_qlzxzh"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:gravity="center"
                    android:padding="@dimen/dimen_5_dp"
                    android:textSize="@dimen/text_size_12_sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="河流中心桩号"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_hlzxzh"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:gravity="center"
                    android:padding="@dimen/dimen_5_dp"
                    android:textSize="@dimen/text_size_12_sp" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_15_dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dimen_10_dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="路线与河流交角"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_lxhljj"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:gravity="center_vertical"
                    android:inputType="number"
                    android:padding="@dimen/dimen_5_dp"
                    android:singleLine="true"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="水流方向"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <CheckedTextView
                    android:id="@+id/tv_slfx"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:drawableRight="@drawable/btn_arrow_selector"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimen_5_dp"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_15_dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dimen_10_dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="汇水面积（m2）"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_hsmj"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:ems="5"
                    android:gravity="center_vertical"
                    android:inputType="numberDecimal"
                    android:padding="@dimen/dimen_5_dp"
                    android:singleLine="true"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="测时水位（深）（m）"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_cssw"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:ems="5"
                    android:gravity="center_vertical"
                    android:inputType="numberDecimal"
                    android:padding="@dimen/dimen_5_dp"
                    android:singleLine="true"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />
            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_15_dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dimen_10_dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="设计洪水位（m）"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_sjhsw"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:ems="5"
                    android:gravity="center_vertical"
                    android:inputType="numberDecimal"
                    android:padding="@dimen/dimen_5_dp"
                    android:singleLine="true"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="通航等级及净空"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <CheckedTextView
                    android:id="@+id/tv_thdjjk"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:drawableRight="@drawable/btn_arrow_selector"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimen_5_dp"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_15_dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dimen_10_dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="最高通航水位"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_zgthsw"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:ems="5"
                    android:gravity="center_vertical"
                    android:inputType="numberDecimal"
                    android:padding="@dimen/dimen_5_dp"
                    android:singleLine="true"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="最低通航水位"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_zdthsw"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:ems="5"
                    android:gravity="center_vertical"
                    android:inputType="numberDecimal"
                    android:padding="@dimen/dimen_5_dp"
                    android:singleLine="true"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />
            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_15_dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dimen_10_dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="被交路名称"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_bjlmc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:gravity="center_vertical"
                    android:maxLength="32"
                    android:padding="@dimen/dimen_5_dp"
                    android:singleLine="true"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="被交路中心桩号"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_bjlzxzh"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:gravity="center"
                    android:padding="@dimen/dimen_5_dp"
                    android:textSize="@dimen/text_size_12_sp" />


            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_15_dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dimen_10_dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="路线与被交路交角"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_lxbjljj"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:gravity="center_vertical"
                    android:inputType="number"
                    android:padding="@dimen/dimen_5_dp"
                    android:singleLine="true"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_holeNumberEdit"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/holeNumber"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp">

                    <EditText
                        android:id="@+id/et_holeNumber"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/shape_input_bg"
                        android:gravity="center"
                        android:hint="@string/kongshu"
                        android:inputType="number"
                        android:maxLength="3"
                        android:padding="@dimen/dimen_5_dp"
                        android:singleLine="true"
                        android:textColor="@color/delect_color"
                        android:textSize="@dimen/text_size_12_sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_10_dp"
                        android:layout_marginRight="@dimen/dimen_10_dp"
                        android:text="/"
                        android:textColor="@color/delect_color"
                        android:textSize="@dimen/text_size_14_sp"
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/et_holeSize"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/shape_input_bg"
                        android:ems="7"
                        android:gravity="center"
                        android:hint="@string/kongjing"
                        android:inputType="numberDecimal"
                        android:padding="@dimen/dimen_5_dp"
                        android:singleLine="true"
                        android:textColor="@color/delect_color"
                        android:textSize="@dimen/text_size_12_sp" />

                </LinearLayout>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_15_dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dimen_10_dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="结构型式"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <CheckedTextView
                    android:id="@+id/tv_jgxs"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:drawableRight="@drawable/btn_arrow_selector"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimen_5_dp"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="交叉角度（°）"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp"
                    android:textStyle="bold" />

                <EditText
                    android:id="@+id/et_jxjd"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen_5_dp"
                    android:background="@drawable/shape_input_bg"
                    android:gravity="center_vertical"
                    android:inputType="number"
                    android:padding="@dimen/dimen_5_dp"
                    android:singleLine="true"
                    android:textColor="@color/delect_color"
                    android:textSize="@dimen/text_size_12_sp" />

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_15_dp"
        android:text="河道（沟、渠）、被交路、交叉管线描述："
        android:textColor="@color/tect_color"
        android:textSize="@dimen/text_size_14_sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/et_hdjlms"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_15_dp"
        android:background="@drawable/shape_input_bg_white"
        android:gravity="left|top"
        android:hint="@string/inputText"
        android:maxLength="256"
        android:maxLines="3"
        android:minHeight="@dimen/dimen_100_dp"
        android:padding="@dimen/dimen_15_dp"
        android:textColor="@color/delect_color"
        android:textSize="@dimen/text_size_14_sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_15_dp"
        android:text="地形、地貌、地质概况及桥型方案控制因素描述："
        android:textColor="@color/tect_color"
        android:textSize="@dimen/text_size_14_sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/et_dxdmkyms"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_15_dp"
        android:background="@drawable/shape_input_bg_white"
        android:gravity="left|top"
        android:hint="@string/inputText"
        android:maxLength="256"
        android:maxLines="3"
        android:minHeight="@dimen/dimen_100_dp"
        android:padding="@dimen/dimen_15_dp"
        android:textColor="@color/delect_color"
        android:textSize="@dimen/text_size_14_sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_15_dp"
        android:text="改河（沟、渠）和改路方案描述："
        android:textColor="@color/tect_color"
        android:textSize="@dimen/text_size_14_sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/et_ghglfa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_15_dp"
        android:background="@drawable/shape_input_bg_white"
        android:gravity="left|top"
        android:hint="@string/inputText"
        android:maxLength="256"
        android:maxLines="3"
        android:minHeight="@dimen/dimen_100_dp"
        android:padding="@dimen/dimen_15_dp"
        android:textColor="@color/delect_color"
        android:textSize="@dimen/text_size_14_sp" />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_45_dp"
        android:layout_marginTop="@dimen/dimen_15_dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="既有涉河工程调查"
            android:textColor="@color/tect_color"
            android:textSize="@dimen/text_size_14_sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/iv_add_project"
            android:layout_width="@dimen/dimen_45_dp"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:scaleType="center"
            android:src="@mipmap/add_work" />
    </RelativeLayout>

    <com.huejie.osmdroid.view.AllShowListView
